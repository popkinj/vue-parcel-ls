{
  "name": "vue-parcel-ls",
  "version": "1.0.1",
  "description": "A Vue demo including Code Splitting, Hot Reloading, ESLint, Vuex, Vue Router, Stylus and Livescript",
  "scripts": {
    "start": "parcel ./public/index.html --open",
    "build": "parcel build ./public/index.html NODE_ENV=production --no-source-maps --public-url ./public --out-dir ./dist",
    "lint": "eslint --ext .js,.vue src"
  },
  "keywords": [
    "Vue",
    "Livescript",
    "Parcel"
  ],
  "author": "Jamie Popkin (popkinj@littleearth.ca)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "babel-polyfill": "^6.26.0",
    "postcss-modules": "^1.4.1",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^4.7.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-livescript": "^1.1.0",
    "pug": "^2.0.4",
    "stylus": "^0.54.5",
    "vue-template-compiler": "^2.6.10"
  }
}
